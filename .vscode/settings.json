{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.idea": true,
        "**/CVS": true,
        "ReverseAutoDiff/LICENSE": true,
        "ReverseAutoDiff/setup.py": true
